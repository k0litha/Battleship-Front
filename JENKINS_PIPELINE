pipeline {
  agent any
    
  tools {nodejs "NodeJs"}
    
  stages {
            
    stage('Build') {
      steps {
        bat 'npm install'

      }
    }  

    stage('Deliver') {
      steps {
        bat 'START npm start'
        input message: 'Finished using the Battleship Server? \n click "Proceed" to stop'
      }
  }
           
  }
}